{
    "manifest_version": 2,
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon128.png"
      }
    },
  
    "permissions": ["storage", "notifications", "contextMenus"]
  }
  